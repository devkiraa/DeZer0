{
  "github": {
    "owner": "devkiraa",
    "repo": "DeZer0",
    "firmwareAssetPatterns": {
      "firmware": "firmware.*\\.bin$",
      "filesystem": "filesystem.*\\.bin$"
    }
  },
  "flash": {
    "baudrate": 115200,
    "flashOptions": {
      "firmware": {
        "address": "0x10000"
      },
      "filesystem": {
        "address": "0x110000"
      },
      "flashSize": "4MB",
      "flashFreq": "40m",
      "flashMode": "dio",
      "eraseAll": true
    }
  }
}
